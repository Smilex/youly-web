		script.
			// Gives ".vertical-right-navbar" the same height as Press Content
			function load() {
				var h = $('#press_design-guidelines-wrapper').height();
				$('#vertical-right-navbar_theFIxed').height(h);
			}
		script.
			// Sets ".vertical-right-navbar" to fixed-top after 300px are scrolled
			$(window).scroll(function(){
				var scroll_top = $(this).scrollTop(); // get scroll position top
				var height_element_parent =  $("#theFixed").parent().outerHeight(); //get high parent element
				var height_element = $("#theFixed").height(); //get high of elemeneto
				var position_fixed_max = height_element_parent - height_element; // get the maximum position of the elemen
				var position_fixed = scroll_top < 325 ? 325 - scroll_top : position_fixed_max > scroll_top ? 0 : position_fixed_max - scroll_top ;
				$("#theFixed").css("top",position_fixed);

				$('#press_design-guidelines-wrapper').find('.section').each(function(i){
					console.log(i);
					if ($(this).position().top <= scroll_top - 300) {
						$('#theFixed').find('a.vertical-right-navbar-active').removeClass('vertical-right-navbar-active');
						$('#theFixed').find('a').eq(i).addClass('vertical-right-navbar-active');
					}
				});
			});