doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible" content="IE=edge")
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
		meta(name="description" content="Neon Admin Panel")
		meta(name="author" content="")

		link(rel="stylesheet" href="/js/neon/jquery-ui/css/no-theme/jquery-ui-1.10.3.custom.min.css")
		link(rel="stylesheet" href="/css/neon/font-icons/entypo/css/entypo.css")
		link(rel="stylesheet" href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic")
		link(rel="stylesheet" href="/css/neon/bootstrap.css")
		link(rel="stylesheet" href="/css/neon/neon-core.css")
		link(rel="stylesheet" href="/css/neon/neon-theme.css")
		link(rel="stylesheet" href="/css/neon/neon-forms.css")
		link(rel="stylesheet" href="/css/neon/custom.css")

		script(src="/js/neon/jquery-1.11.0.min.js")
	body.page-body(data-url="http://neon.dev")
		div.page-container
			div.sidebar-menu
				header.logo-env
					div.logo
						a(href="#")
							img(src="/images/neon/logo@2x.png" width="120")
					div.sidebar-collapse
						a.sidebar-collapse-icon.with-animation(href="#")
							i.entypo-menu
					div.sidebar-mobile-menu.visible-xs
						a.with-animation(href="#")
							i.entypo-menu
				ul#main-menu
					li#search
						form
							input.search-input(type="text")
							button(type="submit")
								i.entypo-search
					li
						a(href="/zoidberg/")
							i.entypo-gauge
							span Dashboard
		// Bottom scripts
		script(src="/js/neon/gsap/main-gsap.js")
		script(src="/js/neon/jquery-ui/js/jquery-ui-1.10.3.minimal.min.js")
		script(src="/js/neon/bootstrap.js")
		script(src="/js/neon/joinable.js")
		script(src="/js/neon/resizeable.js")
		script(src="/js/neon/neon-api.js")
		script(src="/js/neon/neon-custom.js")
		script(src="/js/neon/neon-demo.js")
